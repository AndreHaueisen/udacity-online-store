{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "npm run prettier && nodemon src/server.ts",
    "lint": "eslint ./src --ext .ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "build": "npx tsc",
    "prettier": "prettier --write --config .prettierrc.json \"src/**/*.ts\" \"spec/**/*.ts\"",
    "test": "npm run prettier && NODE_ENV=test db-migrate --env test up && jest --bail=false --detectOpenHandles ; npm run reset_test_db",
    "reset_test_db": "NODE_ENV=test db-migrate --env test reset"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.10.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jasmine": "^3.10.7",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/pg": "^7.14.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.36.0",
    "jasmine": "^3.99.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.6.2"
  }
}
